        </Folder>
        <Folder Name="native" TargetFolderName="native">
          <Folder Name="android" TargetFolderName="android">
            <ProjectItem ReplaceParameters="false" TargetFileName="ant.properties">ant.properties</ProjectItem>
          </Folder>
        </Folder>
        <Folder Name="screens" TargetFolderName="screens">
